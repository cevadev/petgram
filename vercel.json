{
    "version": 2,
    "name":"petgram-barcvilla",
    "builds":[
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },

        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },

        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}